{
  "version": "0.0.18",
  "name": "@navikt/aap-felles-innbygger-react",
  "author": "NAV",
  "homepage": "https://github.com/navikt/aap-felles-innbygger#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/aap-felles-innbygger"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "cjs/index.js",
  "types": "esm/index.d.ts",
  "module": "esm/index.js",
  "files": [
    "/cjs",
    "/esm",
    "/src"
  ],
  "scripts": {
    "clean": "rm -rf ./cjs ./esm",
    "copy-files:esm": "copyfiles -u 1 src/**/*.css esm/",
    "copy-files:cjs": "copyfiles -u 1 src/**/*.css cjs/",
    "build": "npm run clean && tsc -p tsconfig.json && tsc -p tsconfig.esm.json && npm run copy-files:esm && npm run copy-files:cjs"
  },
  "dependencies": {
    "@navikt/ds-css": "1.3.x",
    "@navikt/ds-react": "1.3.x",
    "@navikt/ds-tokens": "1.3.x",
    "prop-types": "^15.8.1",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1"
  }
}
